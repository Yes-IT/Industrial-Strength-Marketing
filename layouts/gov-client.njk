{% extends "layout.njk" %}
{% block body %}
<div class="client-type">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-8 col-sm-offset-2 copy">
                <h2>{{ intro_title if intro_title else title }}</h2>
                <div class="leadin">{{ contents | safe }}</div>
                <div class="text-center"><a href="/free-consultation/" class="btn btn-default">Get a Free Consultation</a></div>
                <br><br>
            </div>
        </div>
    </div>
</div>
<section class="bg-light gov-partners">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-12 text-center">
                <h2>{{partners_section.headline}}</h2>
                <p>
                    {{partners_section.intro}}
                </p>
                <br>
                <div class="logo-flex-grid">
                    {% for item in partners_section.partners %}
                        <img src="{{item.img}}" alt="{{item.title}}"> 
                    {% endfor %}
                </div>
            </div>
        </div>
    </div>
</section>
{% endblock %}